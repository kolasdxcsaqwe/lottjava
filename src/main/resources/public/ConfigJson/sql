CREATE TABLE `v9ym`.`fn_lottery20`  (
  `id` int(11) NOT NULL DEFAULT 10029,
  `roomid` int(11) NOT NULL DEFAULT 10029,
  `gameopen` tinyint(1) NOT NULL DEFAULT 2,
  `da` float NOT NULL DEFAULT 1.96,
  `xiao` float NOT NULL DEFAULT 1.96,
  `dan` float NOT NULL DEFAULT 1.96,
  `shuang` float NOT NULL DEFAULT 1.96,
  `anythree` float NOT NULL DEFAULT 7.3 COMMENT '任意三',
  `anyfour` float NOT NULL DEFAULT 19.66 COMMENT '任意4',
  `frontfourfix` float NOT NULL DEFAULT 9800 COMMENT '前4定位',
  `endfourfix` float NOT NULL DEFAULT 9800 COMMENT '后4定位',
  `frontfourany` float NOT NULL DEFAULT 49.72 COMMENT '前4任意',
  `endfourany` float NOT NULL DEFAULT 49.72 COMMENT '后4任意',
  `touweifix` float NOT NULL DEFAULT 82.34 COMMENT '头尾定位',
  `rules` text CHARACTER SET utf8 COLLATE utf8_general_ci NULL COMMENT '规则',
  `minbet` float NOT NULL DEFAULT 5,
  `maxbet` float NOT NULL DEFAULT 200000,
  `fengtime` int(11) NOT NULL DEFAULT 20,
  PRIMARY KEY (`id`) USING BTREE
) ENGINE = MyISAM CHARACTER SET = utf8 COLLATE = utf8_general_ci ROW_FORMAT = FIXED;


CREATE TABLE `fn_qxcorder` (
`id`  int(11) NOT NULL AUTO_INCREMENT ,
`userid`  text CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL ,
`username`  text CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL ,
`headimg`  text CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL ,
`term`  varchar(50) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL ,
`mingci`  text CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL ,
`content`  text CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL ,
`money`  int(11) NOT NULL ,
`addtime`  datetime NOT NULL ,
`status`  text CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL ,
`jia`  text CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL ,
`roomid`  int(11) NOT NULL ,
`chatid`  varchar(255) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT NULL COMMENT '标识' ,
`gamename`  varchar(20) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT NULL ,
`gamtype`  int(11) NOT NULL DEFAULT 0 ,
`orderamount`  int(11) NOT NULL,
`combineBet`  tinyint(1) NOT NULL DEFAULT 2 ,
PRIMARY KEY (`id`),
INDEX `term` (`term`) USING BTREE ,
INDEX `jssscorderterm` (`term`) USING BTREE
)
ENGINE=MyISAM
DEFAULT CHARACTER SET=utf8 COLLATE=utf8_general_ci
AUTO_INCREMENT=350
ROW_FORMAT=DYNAMIC
;